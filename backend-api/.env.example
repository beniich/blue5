# Environment
NODE_ENV=development
PORT=3000

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/dash1cc_db

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRATION=24h

# Power BI Azure AD
POWERBI_CLIENT_ID=your_azure_ad_client_id
POWERBI_CLIENT_SECRET=your_azure_ad_client_secret
POWERBI_TENANT_ID=your_azure_ad_tenant_id
POWERBI_WORKSPACE_ID=your_powerbi_workspace_id

# Supabase (pour CRM Pro)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your_supabase_service_role_key

# CORS
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:5174,http://localhost:5175

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
